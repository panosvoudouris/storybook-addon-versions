{
  "name": "@arrow-ds/addon-versions",
  "version": "1.0.0",
  "description": "Navigate different versions of storybook style guides.",
  "main": "dist/register.js",
  "scripts": {
    "start": "yarn storybook",
    "storybook": "start-storybook -p 9001 -s ./.storybook",
    "build-storybook": "build-storybook",
    "serve-static": "ws -d storybook-static",
    "lint": "eslint src --ext .js",
    "transpile": "babel ./src --out-dir dist --copy-files --ignore __mocks__,__snapshots__,test.js",
    "test": "jest ./src --coverage",
    "build": "yarn lint && yarn test && yarn transpile",
    "prepublish": "yarn transpile",
    "prepush": "yarn lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buildit/storybook-addon-versions.git"
  },
  "bugs": {
    "url": "https://github.com/buildit/storybook-addon-versions/issues"
  },
  "homepage": "https://github.com/buildit/storybook-addon-versions#readme",
  "keywords": [
    "atomic-design",
    "react",
    "storybook",
    "storybook-addon",
    "storybook-addon-versions",
    "versions"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addons": "^4.1.0",
    "@storybook/react": "^4.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.0.1",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "local-web-server": "^2.6.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^16.8.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6"
  },
  "peerDependencies": {
    "@storybook/addons": "3.1.2",
    "@storybook/react": "3.1.3",
    "prop-types": "^16.8.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "style-loader": "^0.17.0"
  },
  "jest": {
    "coverageDirectory": "reports",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/register.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
